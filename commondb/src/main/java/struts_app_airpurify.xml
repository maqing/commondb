<struts>
	<!-- 画图 -->
	<package name="app_echarts" namespace="/app/echarts" extends="json-default">
		<action name="queryEchartsData" method="queryEchartsData" class="echartsDataAction">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
	</package>

	<!-- 旋转除气库 -->
	<package name="app_airpurify1" namespace="/app/rotatePurify" extends="struts-default">
		<action name="genForm" method="genForm" class="inputPageGenAction">
			<result type="velocity">/app/rotatePurify/new.html</result>
		</action>
		<action name="genSearchPage" method="genSearchPage" class="searchPageGenAction">
			<result type="velocity">/app/rotatePurify/search.html</result>
		</action>
		<action name="saveEntity" method="saveEntity" class="saveEntityAction">
			<result type="velocity">/app/rotatePurify/info.html</result>
		</action>
		<action name="viewEntity" method="viewEntity" class="viewEntityAction">
			<result type="velocity">/app/rotatePurify/view.html</result>
		</action>
		<action name="preEditEntity" method="viewEntity" class="viewEntityAction">
			<result type="velocity">/app/rotatePurify/edit.html</result>
		</action>
		<action name="genManagePage" method="genSearchPage" class="searchPageGenAction">
			<result type="velocity">/app/rotatePurify/manage.html</result>
		</action>
		<action name="preExport" method="genSearchPage" class="searchPageGenAction">
			<result type="velocity">/app/rotatePurify/export.html</result>
		</action>
		<action name="export" method="genSearchPage" class="searchPageGenAction">
			<result >/app/rotatePurify/excel.jsp</result>
		</action>
		<action name="import1" method="step1" class="importAction">
			<result type="velocity">/app/rotatePurify/import1.html</result>
		</action>
		<action name="import2" method="step2" class="importAction">
			<result type="velocity">/app/rotatePurify/import2.html</result>
		</action>
		<action name="import3" method="step3" class="importAction">
			<result type="velocity">/app/rotatePurify/info.html</result>
		</action>
		<action name="preCopyEntity" method="viewEntity" class="viewEntityAction">
			<result type="velocity">/app/rotatePurify/copy.html</result>
		</action>
		<action name="getRelationEntity" method="relationEntity" class="trackEntityAction">
			<result type="velocity">/app/rotatePurify/relationlist.html</result>
		</action>
		<action name="trackEntity" method="trackEntity" class="trackEntityAction">
			<result type="velocity">/app/rotatePurify/relationlist.html</result>
		</action>
	</package>

